{
  "name": "multi-bloc-list",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "examples/multi-bloc-list/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/vite:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/examples/multi-bloc-list"
      },
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production"
        }
      }
    },
    "serve": {
      "executor": "@nrwl/vite:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "multi-bloc-list:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "multi-bloc-list:build:development",
          "hmr": true
        },
        "production": {
          "buildTarget": "multi-bloc-list:build:production",
          "hmr": false
        }
      }
    },
    "test": {
      "executor": "@nrwl/vite:test",
      "outputs": ["{projectRoot}/coverage"],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "{workspaceRoot}/coverage/{projectRoot}"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["examples/multi-bloc-list/**/*.{ts,tsx,js,jsx}"]
      }
    }
  },
  "tags": []
}
